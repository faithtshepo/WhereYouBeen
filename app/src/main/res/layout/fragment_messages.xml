<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/constraintLayout"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".ui.main.PlaceholderFragment">

    <EditText
        android:id="@+id/etFirst"
        android:layout_width="307dp"
        android:layout_height="45dp"
        android:layout_marginStart="63dp"
        android:layout_marginTop="232dp"
        android:layout_marginEnd="41dp"
        android:layout_marginBottom="454dp"
        android:ems="10"
        android:inputType="textPersonName"
        android:singleLine="true"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/textView"
        android:layout_width="299dp"
        android:layout_height="66dp"
        android:layout_marginStart="68dp"
        android:layout_marginTop="142dp"
        android:layout_marginEnd="44dp"
        android:layout_marginBottom="523dp"
        android:text="@string/tvAdminName"
        android:textColor="@color/common_google_signin_btn_text_dark_focused"
        android:textSize="18sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <EditText
        android:id="@+id/etSecond"
        android:layout_width="303dp"
        android:layout_height="51dp"
        android:layout_marginStart="63dp"
        android:layout_marginTop="362dp"
        android:layout_marginEnd="45dp"
        android:layout_marginBottom="318dp"
        android:capitalize="words"
        android:ems="10"
        android:inputType="textPersonName"
        android:singleLine="true"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <Button
        android:id="@+id/btnSubmit1"
        android:layout_width="294dp"
        android:layout_height="41dp"
        android:layout_marginStart="63dp"
        android:layout_marginTop="303dp"
        android:layout_marginEnd="54dp"
        android:layout_marginBottom="387dp"
        android:background="@drawable/buttonstyle2"
        android:text="@string/btnFirst"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <EditText
        android:id="@+id/etNumb"
        android:layout_width="295dp"
        android:layout_height="41dp"
        android:layout_marginStart="63dp"
        android:layout_marginTop="528dp"
        android:layout_marginEnd="53dp"
        android:layout_marginBottom="162dp"
        android:ems="10"
        android:inputType="phone"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <Button
        android:id="@+id/btnSubmit2"
        android:layout_width="290dp"
        android:layout_height="38dp"
        android:layout_marginStart="63dp"
        android:layout_marginTop="451dp"
        android:layout_marginEnd="58dp"
        android:layout_marginBottom="242dp"
        android:background="@drawable/buttonstyle2"
        android:text="@string/btnFirst"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

</androidx.constraintlayout.widget.ConstraintLayout>